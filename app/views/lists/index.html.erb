<div class="list">
<h2>Lists</h2>
</div>
<div>
<% @lists.each do |list| %>
  <div class="card" style="width: 18rem;">
    <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill, class:"card-img-top" %>
    <div class="card-body">
  <h5 class="card-title"><strong><%= link_to list.name, list_path(list) %></strong></h5>
    </div>
  </div>
</div>
<% end %>
<div>

<div>
  <%= link_to "Create List", new_list_path, class: "btn btn-primary" %>
</div>
